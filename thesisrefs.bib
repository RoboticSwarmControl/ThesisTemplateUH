	Keywords = {Monte Carlo methods, assembling, computational complexity, computer vision, industrial robots, materials handling, probability, stability},
	Month = oct,
	Number = {5},
	Pages = {849--857},
	Title = {Part pose statistics: estimators and experiments},
	Volume = {15},
