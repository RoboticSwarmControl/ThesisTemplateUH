	Journal = {The International Journal of Robotics Research},
	Journal1 = {The International Journal of Robotics Research},
	Number = {10},
	Pages = {963--997},
	Title = {Using Partial Sensor Information to Orient Parts},
	Ty = {JOUR},
	Volume = {18},
	Year = {1999},
	Bdsk-File-1 = {},
	Bdsk-Url-1 = {http://ijr.sagepub.com/cgi/content/abstract/18/10/963}}

