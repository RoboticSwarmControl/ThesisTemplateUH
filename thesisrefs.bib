	Isbn = {0167-6911},
	Journal = {Systems \& Control Letters},
	Keywords = {Impacts; Routh model; Nonlinear control; Accessibility; Controllability; Air hockey; Friction; Restitution},
	Month = {4},
	Number = {5},
	Pages = {333--345},
	Title = {Impact controllability of an air hockey puck},
